<article class="quickNav">
  <% if current_user.present? %>
    <ul>
      <%= image_tag @user.gravatar_url%>
        <!-- make this image link to user home -->
      <li>
        <p>Hi, <%= current_user.first_name %><br> Not you?</p>
       <!-- need sign out -->
      </li>
      <div class="verticalRule"></div>
      <li>
        <a class="fa fa-home" id="home"></a>
        <label for="home">home</label>
      </li>
      <div class="verticalRule"></div>
      <li>
        <a class="fa fa-wrench" id="toolbox"></a>
        <label for="toolbox">toolbox</label>
      </li>
      <div class="verticalRule"></div>
      <li>
        <a class="fa fa-bullhorn" id="feedback"></a>
        <label for="feedback">feedback</label>
      </li>
      <div class="verticalRule"></div>
      <li>
        <a class="fa fa-cog" id="settings"></a>
        <label for="settings">settings</label>
      </li>
    </ul>
  <% else %>
    <section>
      <button>
        <%= link_to "Sign up", new_user_registration_path %>
      </button>
      <div class="verticalRule"></div>
      <button>
        <%= link_to "Log in", new_user_session_path %>
      </button>
    </section>
  <% end %>
<article>